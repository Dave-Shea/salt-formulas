# vim: syntax=dosini

[DEFAULT]
lib=
      msg() {
        echo "I: $1"
      }
      git_checkout() {
        (git clone git@github.com:salt-formulas/salt-formula-$1.git $1 || git clone https://github.com/salt-formulas/salt-formula-$1.git $1)
      }
update = git pull --rebase
git_review = git review

[formulas/openvstorage]
checkout = git_checkout openvstorage
skip = lazy

